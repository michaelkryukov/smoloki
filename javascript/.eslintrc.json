{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "standard-with-typescript",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "project": ["tsconfig.json"]
  },
  "rules": {
    "import/order": "error",
    "@typescript-eslint/no-var-requires": "off",
    "@typescript-eslint/comma-dangle": "off"
  }
}
